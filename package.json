{
  "name": "elkulo_feeder",
  "version": "2.0.0",
  "description": "Vue x Slim のRSSフィードリーダー",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "start": "npm run develop & npm run serve",
    "develop": "webpack --mode development --watch",
    "format": "eslint --fix './public/webpack/src/**/*.js'",
    "build": "webpack --mode production",
    "serve": "php -S localhost:8000 -t ./public",
    "composer:install": "cd feeder/core/ && composer install",
    "composer:update": "cd feeder/core/ && composer update",
    "composer:analyse": "cd feeder/core/ && composer analyse",
    "composer:format": "cd feeder/core/ && composer format",
    "deploy": "sh deploy.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-loader": "^8.2.5",
    "core-js": "^3.22.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.14.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "typescript": "^4.6.4",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "axios": "^0.27.2",
    "bootstrap": "^5.1.3",
    "smoothscroll-polyfill": "^0.4.4",
    "vue": "^3.2.33"
  }
}
