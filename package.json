{
  "name": "elkulo_feeder",
  "version": "1.0.0",
  "description": "Built locally with Webpack",
  "private": true,
  "scripts": {
    "start": "npm run develop & npm run serve",
    "develop": "webpack --mode development --watch",
    "format": "eslint --fix './public/webpack/src/**/*.js'",
    "build": "webpack --mode production",
    "serve": "php -S localhost:8000 -t ./public",
    "composer:update": "cd ./slim/ && composer update",
    "composer:lint": "cd ./slim/ && composer phpcs",
    "composer:format": "cd ./slim/ && composer phpcbf"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "babel-loader": "^8.2.2",
    "core-js": "^3.13.1",
    "css-loader": "^5.2.6",
    "css-minimizer-webpack-plugin": "^3.0.1",
    "eslint": "^7.27.0",
    "eslint-webpack-plugin": "^2.5.4",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^2.3.0",
    "sass": "^1.34.0",
    "sass-loader": "^11.1.1",
    "typescript": "^4.3.2",
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "bootstrap": "^5.0.1",
    "vue": "^2.6.12"
  }
}
