{
  "name": "elkulo_feeder",
  "version": "1.0.0",
  "description": "Vue x Slim のRSSフィードリーダー",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "start": "npm run develop & npm run serve",
    "develop": "webpack --mode development --watch",
    "format": "eslint --fix './public/webpack/src/**/*.js'",
    "build": "webpack --mode production",
    "serve": "php -S localhost:8000 -t ./public",
    "composer:install": "cd feeder/core/ && composer install",
    "composer:update": "cd feeder/core/ && composer update",
    "composer:phpcs": "cd feeder/core/ && composer phpcs",
    "composer:phpcbf": "cd feeder/core/ && composer phpcbf",
    "composer:phpstan": "cd feeder/core/ && composer phpstan",
    "deploy": "sh deploy.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.4",
    "core-js": "^3.22.0",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "eslint": "^8.13.0",
    "eslint-webpack-plugin": "^3.1.1",
    "mini-css-extract-plugin": "^2.6.0",
    "prettier": "^2.6.2",
    "sass": "^1.50.0",
    "sass-loader": "^12.6.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.5",
    "axios": "^0.26.1",
    "bootstrap": "^5.1.3",
    "smoothscroll-polyfill": "^0.4.4",
    "vue": "^2.6.14"
  }
}
