{
  "name": "elkulo_feeder",
  "version": "2.1.0",
  "description": "Vue x Slim のRSSフィードリーダー",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "start": "npm run develop & npm run serve",
    "develop": "webpack --mode development --watch",
    "format": "eslint --fix './public/webpack/src/**/*.js'",
    "build": "webpack --mode production",
    "serve": "php -S localhost:8000 -t ./public",
    "composer:install": "cd feeder/core/ && composer install",
    "composer:update": "cd feeder/core/ && composer update",
    "composer:analyse": "cd feeder/core/ && composer analyse",
    "composer:format": "cd feeder/core/ && composer format",
    "deploy": "sh deploy.sh"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "core-js": "^3.27.1",
    "css-loader": "^6.7.3",
    "css-minimizer-webpack-plugin": "^4.2.2",
    "eslint": "^8.31.0",
    "eslint-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^2.7.2",
    "prettier": "^2.8.2",
    "sass": "^1.57.1",
    "sass-loader": "^13.2.0",
    "typescript": "^4.9.4",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.6",
    "axios": "^1.2.2",
    "bootstrap": "^5.2.3",
    "smoothscroll-polyfill": "^0.4.4",
    "vue": "^3.2.45"
  }
}
