{
  "name": "elkulo_feeder",
  "version": "1.0.0",
  "description": "Built locally with Webpack",
  "private": true,
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json && npm install",
    "start": "npm run develop & npm run serve",
    "develop": "webpack --mode development --watch",
    "format": "eslint --fix './public/webpack/src/**/*.js'",
    "build": "webpack --mode production",
    "serve": "php -S localhost:8000 -t ./public",
    "composer:update": "cd ./slim/ && composer update",
    "composer:lint": "cd ./slim/ && composer phpcs",
    "composer:format": "cd ./slim/ && composer phpcbf"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "babel-loader": "^8.2.2",
    "core-js": "^3.16.0",
    "css-loader": "^6.2.0",
    "css-minimizer-webpack-plugin": "^3.0.2",
    "eslint": "^7.31.0",
    "eslint-webpack-plugin": "^3.0.1",
    "mini-css-extract-plugin": "^2.1.0",
    "prettier": "^2.3.2",
    "sass": "^1.36.0",
    "sass-loader": "^12.1.0",
    "typescript": "^4.3.5",
    "webpack": "^5.47.1",
    "webpack-cli": "^4.7.2"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "axios": "^0.21.1",
    "bootstrap": "^5.0.2",
    "smoothscroll-polyfill": "^0.4.4",
    "vue": "^2.6.14"
  }
}
